
@{
    ViewBag.Title = "Race Master Report";
    Layout = "~/Views/Shared/_Report.cshtml";
}
<table class="A4">
    <tr>
        <td align="left"> <strong>Report Name: </strong> PS-07 </td>
        <td>
            <div style="text-align:right">
                Print Date:  @ViewBag.date <br />
                Print Time:  @ViewBag.time <br />
            </div>
        </td>
    </tr>
</table>
<br /><br />
<div style="text-align:center" class="A4 title">
    <div>PERBADANAN LABUAN </div>
    <div>RACE LISTING </div>
</div>
<br /><br />
<table class="A4">
    <tr>
        <th> No </th>
        <th> Race Code </th>
        <th> Description </th>
    </tr>
    @foreach (var item in ViewBag.races)
    {
        <tr>
            <td>@item.RACEID</td>
            <td>@item.RACE_CODE</td>
            <td>@item.RACE_DESC</td>
        </tr>
    }
</table>


