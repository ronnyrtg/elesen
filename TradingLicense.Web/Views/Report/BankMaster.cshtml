
@{
    ViewBag.Title = "Bank Master Report";
    Layout = "~/Views/Shared/_Report.cshtml";
}
<table class="A4">
    <tr>
        <td align="left"> <strong>Report Name: </strong> LS-06 </td>
        <td>
            <div style="text-align:right">
                Print Date:  @ViewBag.date <br />
                Print Time:  @ViewBag.time <br />
            </div>
        </td>
    </tr>
</table>
<br /><br />
<div style="text-align:center" class="A4 title">
    <div>PERBADANAN LABUAN </div>
    <div>BANK LISTING </div>
</div>
<br /><br />
<table class="A4">
    <tr>
        <th> No </th>
        <th> Bank Code </th>
        <th> Bank Names </th>
        <th> Remarks </th>
    </tr>
    @foreach (var item in ViewBag.banks)
    {
        <tr>
            <td>@item.BANKID</td>
            <td>@item.BANK_CODE</td>
            <td>@item.BANK_NAME</td>
            <td>@item.BANK_REMARKS</td>
        </tr>
    }
</table>


