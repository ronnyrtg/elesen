@using (Html.BeginForm("PaymentInquiryPaidPdf", "Report", FormMethod.Get))
{
    <div class="form-group">
        <label for="licenseCode"> License Code </label>
        @Html.DropDownList("LicenseCode", null, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label for="fromdate"> From Transaction Date </label>
        <input type="date" name="fromdate" class="form-control" />
    </div>
    <div class="form-group">
        <label for="todate"> To Transaction Date </label>
        <input name="todate" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label for="licenseNo"> License No </label>
        <input name="licenseNo" type="text" class="form-control" />
    </div>
    <div class="form-group">
        <label for="receiptNo"> Receipt No </label>
        <input name="receiptNo" type="text" class="form-control" />
    </div>
    <div class="form-group">
        <label for="receiptRef"> Receipt Reference </label>
        <input name="receiptRef" type="text" class="form-control" />
    </div>
    <div class="form-group">
        <label for="accountNo"> Account No </label>
        <input name="accountNo" type="text" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
}



